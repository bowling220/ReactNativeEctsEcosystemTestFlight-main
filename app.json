{
  "expo": {
    "name": "EctsEcosystem",
    "slug": "EctsEcosystem",
    "version": "1.0.2",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "msauth",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ectscmp.EctsEcosystem",
      "buildNumber": "12",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["msauth.com.ectscmp.EctsEcosystem"]
          }
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "sound": "notification.mp3"
      },
      "permissions": ["INTERNET"],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "msauth",
            "host": "com.ectscmp.EctsEcosystem",
            "pathPrefix": "/auth"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c2c2013c-7df7-4614-9621-d077e28bed30"
      }
    },
    "owner": "ectscmp"
  }
}
